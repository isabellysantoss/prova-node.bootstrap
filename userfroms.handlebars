<body>


    <div class="container">
    <div class="row">


        <div class="col-6">
            <h1>Cadastrar Produtos</h1>
            <form>
                <div class="form-group">
                    <label for="nome">Nome do Produto:</label>
                    <input class="form-control"type="text" name="nome"id="nome">
                </div>

                <div class="form-group">
                    <label for="nome"> Nome Fornecedor:</label>
                    <input class="form-control"name="Fornecedor" >
                </div>

                <div class="form-group">
                    <label for="nome">Data da Compra:</label>
                    <input class="form-control"input="date"type="data" name="data"id="nome">
                </div>

                <div class="form-group">
                    <label for="nome">Valor da Compra:</label>
                    <input class="form-control"type="valor"name="valor"id="valor">
                </div>

                <div class="form-group">
                    <label for="nome">Valor de Venda:</label>
                    <input class="form-control"type="valor" name="valor"id="valor">
                </div>

                <div class="form-group">
                    <label for="nome">Categoria:</label>
                    <input class="form-control"type="categoria" name="categoria"id="categoria">
                </div>

                <div class="form-group text-center">
                    <button type="submit" class="btn btn-primary">ENVIAR</button>

            </form>
        </div>
    </div>
    
    
</body>
